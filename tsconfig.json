{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": ["@cloudflare/workers-types"],
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "lib": [
      "ESNext"
    ],
    "jsx": "react-jsx",
